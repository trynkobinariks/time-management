{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "bracketSameLine": false,
  "embeddedLanguageFormatting": "auto",
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "htmlWhitespaceSensitivity": "css",
  "vueIndentScriptAndStyle": false,
  "useTabs": false,
  "singleAttributePerLine": false,
  "overrides": [
    {
      "files": "*.{ts,tsx,js,jsx}",
      "options": {
        "objectPattern": {
          "multiline": true,
          "consistent": true
        }
      }
    }
  ]
}
